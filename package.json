{
  "name": "log.theater",
  "version": "0.0.1",
  "private": true,
  "description": "onesword0618's blog",
  "license": "MIT",
  "author": "onesword0618",
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "ts-node ./node_modules/.bin/gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "typedoc": "typedoc --hideGenerator --out ./docs/ ./src/",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "format:fix": "prettier -w './src/**/*.{ts,tsx}'",
    "format": "prettier -c './src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "gatsby": "^3.9.1",
    "gatsby-image": "^3.10.0",
    "gatsby-plugin-gatsby-cloud": "^2.9.1",
    "gatsby-plugin-image": "^1.10.1",
    "gatsby-plugin-sharp": "^3.10.2",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-source-filesystem": "^3.10.0",
    "gatsby-transformer-sharp": "^3.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.3",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-cli": "^3.9.0",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "simple-git-hooks": "^2.5.1",
    "tslib": "^1.14.1",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "preserveUnused": true
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint",
      "npm run format",
      "npm run typedoc"
    ]
  }
}
